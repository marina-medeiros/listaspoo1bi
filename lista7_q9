using System;

class MainClass {
  public static void Main (string[] args) {
    Console.WriteLine ("Digite uma frase com trÃªs palavras:");
    string frase = Console.ReadLine();
    string[] f = frase.Split(' ');
    string p1 = (f[0]);
    string p2 = (f[1]);
    string p3 = (f[2]);

    char l1 = p1[0];
    char l2 = p2[0];
    char l3 = p3[0];

    Console.WriteLine($"{l1}{l2}{l3}");
  }
}
